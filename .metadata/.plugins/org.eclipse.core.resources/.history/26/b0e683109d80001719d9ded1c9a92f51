package codeground;

import java.util.Scanner;

//25ë²ˆ 3N+1 
public class Sol25 {
	static int Answer;

	public static void main(String args[]) throws Exception	{
		Scanner sc = new Scanner(System.in);
		//Scanner sc = new Scanner(new FileInputStream("input.txt"));

		int T = sc.nextInt();
		for(int test_case = 0; test_case < T; test_case++) {

			Answer = 0;
			
			int k = sc.nextInt();
			
			int minNumber = (int) Math.pow(2, k);
			int maxNumber = (int) Math.pow(2, k);
			
			int temp = maxNumber;
			int count = 0;
			
			while(temp != 1) {
				count++;
				temp /= 2;
				
				if(temp == 4) {
					break;
				}
				
				if( (temp - 1) % 3 != 0) {
					continue;
				} else {
					int val = temp;
					//(temp - 1) / 3;
					
					while(count != 0) {
						count--;
						
						if( (val-1) % 3 == 0) {
							val = (val-1) / 3;
						} else {
							val = val*2;
						}
						System.out.println(val);
					}
					
				}
				
				/*
				
				if( (temp - 1) % 3 != 0) {
					temp = temp / 2;
					continue; 
				} else {
					int val = temp;
					
					while(count != 0) {
						
						
						
						if( (val - 1) % 3 == 0 && val != 4) {
							val = (val - 1) / 3;
						} else {
							val = val*2;
						}
						count--;
					}
				}
				*/
			}

			System.out.println("Case #"+(test_case+1));
			System.out.println(minNumber + " " + maxNumber);
		}
	}


}
