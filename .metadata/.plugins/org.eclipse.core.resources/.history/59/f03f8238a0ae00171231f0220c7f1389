package SS;

import java.util.Scanner;

public class Main {
	static int Ans = 0;
	static int Len = 4;
	static int [][] map;
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int col = sc.nextInt();
		int row = sc.nextInt();
		
		map = new int[row][col];
		Ans = 0;
		
		for(int i=0; i<col ; i++)
			for(int j=0 ; j<row ; j++) 
				map[j][i] = sc.nextInt();
			
		for(int i=0 ; i<col ; i++) {
			for(int j=0; j<row ; j++) {
				dfs(0, j , i, 0);
			}
		}
		
		
		
		sc.close();
	}
	
	public static void dfs(int depth, int x, int y, int sum) {
		if(depth == Len - 1) {
			sum += map[x][y];
			
			Ans = Math.max(sum, Ans);
			return;
		}
	}
}
