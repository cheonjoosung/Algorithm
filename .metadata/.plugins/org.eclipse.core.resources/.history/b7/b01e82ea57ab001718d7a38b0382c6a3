package backjun.lec03;

import java.util.Scanner;

public class P10971 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int [][] arr = new int[n][n];
		
		for(int i=0 ; i<n ; i++) {
			for(int j=0 ; j<n ; j++) {
				arr[j][i] = sc.nextInt();
			}
		}
		
		sc.close();
	}
}
