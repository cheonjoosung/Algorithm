package SS;

import java.util.Scanner;


public class Main {
	static int [][] map;
	static int Ans;
	static int Blank = 0;
	static int Wall = 1;
	static int Red = 2;
	static int Blue = 3;	
	static int End = 4;	

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		int col = sc.nextInt();
		int row = sc.nextInt();

		map = new int[row][col];
		
		for(int i=0 ; i<col ; i++)
			for(int j=0 ; j<row ; j++) {
				String s = sc.nextLine();
				for(int k=0 ; k<s.length() ; k++) {
					System.out.println(j + " " + i + " " + k);
					if(s.charAt(k) == '#') map[j][i] = Wall;
					else if(s.charAt(k) == '.') map[j][i] = Blank;
					else if(s.charAt(k) == 'R') map[j][i] = Red;
					else if(s.charAt(k) == 'B') map[j][i] = Blue;
					else	 if(s.charAt(k) == 'O') map[j][i] = End;
				}
			}

		for(int [] temp : map){
			for(int val :temp) {
				System.out.print(val + " ");
			}
			System.out.println();
		}
		
		sc.close();
	}

}
