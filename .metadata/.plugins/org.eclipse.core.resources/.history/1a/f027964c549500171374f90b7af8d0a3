package codeground;

import java.util.Arrays;
import java.util.Scanner;

public class Test {
	
	public static void main(String args[]) throws Exception	{
	    Scanner sc = new Scanner(System.in);
	    
	 
	    long result = fact(10);
	    System.out.println(result);
	    System.out.println(gcd(16, 10));
	    System.out.println(lcd(16, 10));
	}
	

    public int[] solution(int[][] v) {
        int[] answer = {};
        
        int x = v[0][0];
        int y = v[0][1];
        
        int findX = -1;
        int findY = -1;
        
        for(int i=1 ; i < v.length ; i++) {
            if(x == v[i][0]) {
              findX = i;
            }
            
            if(y == v[i][1]) {
              findY = i;
            }
        }
                switch(findX) {
            case -1 :
                answer[0] = x;
                break;
            case 1 :
                answer[0] = v[findX+1][0];
                break;
            case 2 : 
                answer[0] = v[findX-1][0];
                break;
        }
        
         switch(findY) {
            case -1 :
                answer[1] = y;
                break;
            case 1 :
                answer[1] = v[findY+1][1];
                break;
            case 2 : 
                answer[1] = v[findY-1][1];
                break;
        }
        

        return answer;
    }
}